</head>


<body>

    <% include ./header %>
    <% include ./linknav %>

    <main data-role="main">


    <div class="content-left">
        <div class="in-calender">
            <div class="in-calender-year year">
            <span></span>
            </div>
            <div class="in-calender-month month">
            <span></span>
            </div>
        </div>


        <div class="side-calender-container">
